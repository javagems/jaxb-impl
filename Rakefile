require 'rubygems'
require 'rake'

begin
  require 'jeweler'
  Jeweler::Tasks.new do |gem|
    gem.name = "jaxb-impl"

    gem.summary = %Q{A JavaGem version of jaxb-impl - packaged by JavaGems (http://www.javagems.org)}



    gem.description = %Q{jaxb-impl packaged up in a JavaGem}

    gem.homepage = nil

    gem.authors = ["pending/unknown"]

    gem.email = "autobuild@javagems.org"
    gem.version = "2.1.10"

  

    gem.add_development_dependency "jaxb-api", "~>2.1"

  end
rescue LoadError
  puts "Jeweler (or a dependency) not available. Install it with: sudo gem install jeweler"
end

task :default => :build
